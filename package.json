{
  "name": "kilangdm-dashboard",
  "version": "1.0.0",
  "description": "Enhanced Analytics Dashboard for KilangDM with Firebase integration",
  "main": "public/dashboard.html",
  "scripts": {
    "start": "firebase serve",
    "build": "node build.js build",
    "build:prod": "NODE_ENV=production node build.js build",
    "clean": "node build.js clean",
    "deploy": "npm run build && firebase deploy",
    "deploy:prod": "node deploy-production.js && firebase deploy",
    "production-ready": "node deploy-production.js",
    "dev": "firebase serve --port 5000",
    "prod": "firebase deploy --only hosting",
    "test": "node test-system.js",
    "lint": "echo 'Linting process completed'"
  },
  "keywords": [
    "dashboard",
    "analytics",
    "firebase",
    "ecommerce",
    "marketing",
    "sales",
    "kilangdm"
  ],
  "author": "KilangDM Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kilangdm/dashboard.git"
  },
  "bugs": {
    "url": "https://github.com/kilangdm/dashboard/issues"
  },
  "homepage": "https://github.com/kilangdm/dashboard#readme",
  "devDependencies": {
    "tailwindcss": "^4.1.11"
  },
  "dependencies": {},
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
